{
  "extends": "@audio-ui/typescript/nextjs",
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "audio-ui/*": [
        "./src/*"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ]
}